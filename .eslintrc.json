{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {"ecmaVersion": 2020, "ecmaFeatures": {"jsx": true }, "sourceType": "module"},
  "rules": {
    "prettier/prettier": "warn"
  }
}
